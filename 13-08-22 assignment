#include<stdio.h>
int main()
{
    int i,j,n;
    char ch='A',ch1='a';
    printf("enter n value : ");
    scanf("%d",&n);
    for(i=1; i<=n; i++){
        for(j=1; j<=i; j++){
          printf("%c%c\t",ch++,ch1++);
        }
        printf("\n");
    }
    printf("\n pattern is printed");
    return 0;
}
