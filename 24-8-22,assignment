#include <stdio.h>  
int main()  
{    
int i, j, n=6, k;  
printf (" Enter a n value: ");  
scanf("%d", &n);   
printf("\n");  
for (i = 1; i <= n; i++)   
{  
        for (j = 1; j <= i; j++)   
             {  
                 printf("  ");   
           }  
        for (k = i; k <=n ; k++)  
            {  
               printf(" %d", k);   
           }  
          printf ("\n");   
 }  
return 0;      
}  
