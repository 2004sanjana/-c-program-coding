int main()
{
	int a[24][24];
	int i,j,r,c,sum;
	printf("enter how many rows\n");
	scanf("%d",&r);
	printf("enter how many cols\n");
	scanf("%d",&c);
	printf("enter elements into array\n");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			sum=sum+a[j][i];
		}
		printf("%d col Sum = %d \n",i+1,sum);
		sum=0;
			}
}
